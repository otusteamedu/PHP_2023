# Временная зона
WORKSPACE_TIMEZONE=Europe/Moscow

# Настройки Nginx
# Порт, который следует использовать
# для соединения с локального компьютера
NGINX_PORT=80

# Настройки Redis
# Порт, который следует использовать
# для соединения с локального компьютера
REDIS_PORT=6379

MYSQL_DATABASE=
MYSQL_ROOT_PASSWORD=

# Настройки MySQL 5.7.x
# Порт, который следует использовать
# для соединения с локального компьютера
MYSQL_PORT=3306

# Настройки PHP 8.2
# Внешний порт, доступен с локального компьютера
PHP_8_2_PORT=9004

# Настройка memcached
# Порт
MEMCACHED_PORT=11211