{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": ["framework", "laravel"],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": "^8.2.0",
    "ext-zip": "*",
    "barryvdh/laravel-dompdf": "^2.0",
    "centrifugal/phpcent": "~3.0",
    "cviebrock/laravel-elasticsearch": "^9.0",
    "doctrine/dbal": "^3.4",
    "gkarman/otus-test-package": "^1.0",
    "gmostafa/php-graphql-client": "^1.13",
    "hflabs/dadata": "^22.3",
    "inspector-apm/inspector-laravel": "^4.7",
    "jurchiks/numbers2words": "^2.7",
    "laravel/framework": "^9.0",
    "laravel/horizon": "^5.10",
    "laravel/tinker": "^2.0",
    "league/flysystem-aws-s3-v3": "^3.2",
    "league/flysystem-ziparchive": "^3.2",
    "maatwebsite/excel": "^3.1",
    "mews/purifier": "^3.4",
    "pda/pheanstalk": "^4.0",
    "predis/predis": "~1.0",
    "rebing/graphql-laravel": "^8.3",
    "sentry/sentry-laravel": "^2.13",
    "spatie/laravel-ignition": "^1.0",
    "tymon/jwt-auth": "dev-develop#34d8e48 as 1.0.0-rc.3.2",
    "webklex/laravel-imap": "2.4"
  },
  "require-dev": {
    "filp/whoops": "^2.0",
    "fzaninotto/faker": "^1.4",
    "mockery/mockery": "^1.0",
    "nunomaduro/collision": "^6.1",
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "classmap": [
      "database/seeds",
      "database/factories"
    ],
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": [
      ]
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.exle', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true,
    "allow-plugins": {
      "php-http/discovery": false
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
