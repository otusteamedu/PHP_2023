{
    "name": "alifcapital/alifshop_package",
    "description": "SuperApp Package",
    "keywords": [
        "SuperApp"
    ],
    "homepage": "https://github.com/alifshop/alifshop-package",
    "license": "MIT",
    "authors": [
        {
            "name": "Mehrubon",
            "email": "husaynov888@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "psr-4": {
            "Sekaiichi\\SuperApp\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sekaiichi\\SuperApp\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
