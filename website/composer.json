{
  "name": "dimonvelsk/otus",
  "type": "project",
  "require": {
    "vlucas/phpdotenv": "^5.5",
    "elasticsearch/elasticsearch": "^8.9",
    "symfony/console": "^6.3"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Dima Teplukhin"
    }
  ],
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "minimum-stability": "stable",
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.7"
  },
  "scripts": {
    "phpcs": "phpcs --standard=PSR2"
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true
    }
  }
}
