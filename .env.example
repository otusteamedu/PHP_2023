SHORT_NAME=otus
APPLICATION_ROOT=./:/var/www/html

NGINX_VERSION=1.23.3
NGINX_PORT=80
NGINX_HOST=application.local
NGINX_ROOT=/var/www/html
NGINX_CONF=./.docker/nginx/templates/:/etc/nginx/templates

PHP_UID=1000
PHP_CONTEXT=./.docker/php
PHP_SOCKET_PATH=/var/run/
PHP_EXTRA_HOST=host.docker.internal:host-gateway
PHP_INI=./.docker/php/conf.d/php.ini:/usr/local/etc/php/conf.d/php.ini
PHP_FPM_CONF=.docker/php/php-fpm.d/zz-docker.conf:/usr/local/etc/php-fpm.d/zz-docker.conf

REDIS_VERSION=latest
REDIS_PORT=6379

MEMCACHED_PORT=11211
MEMCACHED_VERSION=latest

MYSQL_VERSION=latest
MYSQL_ROOT_PASSWORD=root
MYSQL_DATABASE=loc
MYSQL_PORT=3306
MYSQL_USER=dev
MYSQL_PASSWORD=123456
MYSQL_DB=db:/var/lib/mysql
MYSQL_CONF=./.docker/mysql/my.cnf:/etc/mysql/my.cnf

PHP_CLI_CONTEXT=./.docker/cli